program Demo;

uses
  System.StartUpCopy,
  FMX.Forms,
  view.principal in 'view.principal.pas' {Form1},
  view.lista in 'view.lista.pas' {Form2},
  frame.cliente in 'frame.cliente.pas' {Frame1: TFrame};

